package j0219;

public class Op3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		// 관계 연산자 (비교 연산자)  대소비교..
		// 논리 연산자 : 참 거짓을 판별할 수 있는 연산자(boolean), && || ^
	
		
		System.out.println("관계, 논리연산자");
		
		int i = 0; int j = 5;
		if (i<j) {
			System.out.println("참");
		}else {
			System.out.println("거짓");
		}
		
		

	}

}
